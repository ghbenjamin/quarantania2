<exporter version="1.0">
  <name>qua_tiled</name>
  <nameAlias>quatiledexporter</nameAlias>
  <displayName>qua_tiled</displayName>
  <description>Exporter for qua Engine</description>
  <version>1.0</version>
  <files>
    <file>
      <name>quajson_tiled</name>
      <displayName>qua tiled (json)</displayName>
      <fileExtension>json</fileExtension>
      <description>JSON data file for qua Engine</description>
      <template>qua.json</template>
    </file>
  </files>
  <supportsTrimming>false</supportsTrimming>
  <supportsRotation>no</supportsRotation>
  <rotationDirection>cw</rotationDirection>
  <supportsNPOT>true</supportsNPOT>
  <supportsTrimSpriteNames>true</supportsTrimSpriteNames>
  <supportsTextureSubPath>no</supportsTextureSubPath>
  <supportsPivotPoint>true</supportsPivotPoint>
  <supportsScale9>true</supportsScale9>
</exporter>
